//removing error

def find_average(mark_list):
	total=0
	try:
	    for i in range(0, len(mark_list)):
		    total+=mark_list[i]
	    marks_avg=total/len(mark_list)
	    return marks_avg
	except NameError:
	    print("NameError")
	except ZeroDivisionError:
	    print("by zero")
	except TypeError:
	    print("type")
try:
    m_list=[1,2,3,4]
    print("Average marks:", find_average(m_list))
except NameError:
    print("NameError")
    
